          {
            "name": "Get Pet by Status",
            "request": {
              "method": "GET",
              "header": [],
              "url": {
                "raw": "https://petstore.swagger.io/v2/pet/findByStatus?status=available",
                "protocol": "https",
                "host": ["petstore", "swagger", "io"],
                "path": ["v2", "pet", "findByStatus"],
                "query": [
                  {
                    "key": "status",
                    "value": "available"
                  }
                ]
              }
            },
            "response": [],
            "event": [
              {
                "listen": "test",
                "script": {
                  "exec": [
                    "// Test 1: Validate successful 200 response",
                    "pm.test('Status code is 200', function () {",
                    "    pm.response.to.have.status(200);",
                    "});",
                    "",
                    "// Test 2: Validate response is array",
                    "pm.test('Response is array', function () {",
                    "    pm.expect(pm.response.json()).to.be.an('array');",
                    "});",
                    "",
                    "// Test 3: Validate required fields in pet objects",
                    "pm.test('Response contains required pet fields', function () {",
                    "    const pets = pm.response.json();",
                    "    if (pets.length > 0) {",
                    "        const pet = pets[0];",
                    "        pm.expect(pet).to.have.property('id');",
                    "        pm.expect(pet).to.have.property('name');",
                    "        pm.expect(pet).to.have.property('status');",
                    "        pm.expect(pet).to.have.property('photoUrls');",
                    "    }",
                    "});",
                    "",
                    "// Test 4: Validate status field contains correct value",
                    "pm.test('All pets have correct status', function () {",
                    "    const pets = pm.response.json();",
                    "    pets.forEach(pet => {",
                    "        pm.expect(pet.status).to.equal('available');",
                    "    });",
                    "});",
                    "",
                    "// Test 5: Validate photoUrls is array",
                    "pm.test('photoUrls field is array', function () {",
                    "    const pets = pm.response.json();",
                    "    if (pets.length > 0) {",
                    "        pm.expect(pets[0].photoUrls).to.be.an('array');",
                    "    }",
                    "});"
                  ],
                  "type": "text/javascript"
                }
              }
            ]
